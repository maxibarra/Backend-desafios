    "code": "\"use strict\";\n\nvar express = require('express');\n\nvar fs = require('fs');\n\nvar contadorItems = 0;\nvar contadorRandom = 0;\nvar app = express();\nvar server = app.listen(8080, function () {\n  console.log('escuchando en el puerto 8080');\n});\napp.get(\"/\", function (req, res) {\n  res.send(\"Bienvenidos\");\n});\napp.get('/items', function (req, res) {\n  contadorItems++;\n  fs.promises.readFile('./productos.json').then(function (data) {\n    return data.toString('utf-8');\n  }).then(function (datos) {\n    res.send(datos);\n  });\n});\napp.get('/item-random', function (req, res) {\n  contadorRandom++;\n\n  var aleatorio = function aleatorio(min, max) {\n    return Math.floor(Math.random() * (max - min)) + min;\n  };\n\n  fs.promises.readFile('./productos.json').then(function (data) {\n    return data.toString('utf-8');\n  }).then(function (datos) {\n    var json = JSON.parse(datos);\n    res.json({\n      item: json[aleatorio(0, json.length - 1)]\n    });\n  });\n});\napp.get('/visitas', function (req, res) {\n  res.send(\"<p>la cantidad de visitas de item random es de \".concat(contadorRandom, \",</p><br>\\n    la cantidad de visitas de item es de \").concat(contadorItems));\n});\n//# sourceMappingURL=data:application/json;charset=utf-8;base64,",
